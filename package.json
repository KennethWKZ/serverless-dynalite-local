{
  "name": "serverless-dynalite2-local",
  "version": "0.3.1",
  "description": "Serverless plugin to run DynamoDB locally with dynalite.",
  "main": "lib/index.js",
  "repository": "https://github.com/KennethWKZ/serverless-dynalite-local",
  "author": "Håkon Åmdal (hakon@aamdal.com)",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "^15.0.3",
    "ava": "^3.15.0",
    "np": "^7.6.1",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "watch": "tsc -p . --watch",
    "build": "tsc -p .",
    "lint": "prettier --check src/*",
    "test": "ava",
    "deploy": "np"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "dynalite": "^3.2.2"
  },
  "files": [
    "lib/**/*.{js,js.map}"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
